   function LoadAjaxGetHotelRelationInfo() {
       var scrollWaiter = {
           disabled : false,
           _requestCallback : false,
           _eventCallback : false,
           start: function (delay, targetElementId, requestCallback) {
               if (targetElementId && requestCallback) {
                   this._requestCallback = requestCallback;
                   this._bindEvents(delay, targetElementId);
               }
           },
           _bindEvents: function (delay, targetElementId) {
               var self = this,
                   timeout = null;

               var callbackFn = function () {
                   var $target = $(targetElementId);
                   if (self._isComingUp($target)) {
                       self._request();
                   }
               };

               var eventFn = function () {
                   clearTimeout(timeout);
                   timeout = setTimeout(callbackFn, delay || 200);
               };

               this._eventCallback = eventFn;

               $(window).bind('scroll', eventFn);
               $(window).bind('resize', eventFn);
           },
           _request: function () {
               if (this.disabled) {
                   return;
               }
               this.disabled = true;
               $(window).unbind('scroll', this._eventCallback);
               $(window).unbind('resize', this._eventCallback);

               this._requestCallback && this._requestCallback();
           },
           _isComingUp: function (el) {
               var offset = el.offset(),
                   docOffsetTop = document.documentElement.scrollTop || document.body.scrollTop,
                   viewAreaHeight = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;
               return offset.top <= docOffsetTop + viewAreaHeight + 500;
           }
       };

       var relatePanel = (hotelDomesticConfig.bookingOver || hotelDomesticConfig.bookOverView) ? '#AdviceHotelHasPrice2' : '#AdviceHotelHasPrice';
        var relationInfoAjax = function() {
            $.ajax(addressUrlConfig.AjaxGetHotelRelationInfo, {
                cache: true,
                onsuccess: function (response) {
                    if (response && response.responseText) {
                        if (hotelDomesticConfig.isghi == "T" && $('#adviseHotel').length > 0) {
                            $('#adviseHotel').html(response.responseText);
                            $(relatePanel).html('');
                        } else {
                            $(relatePanel).html(response.responseText);
                        }
                    }
                }
            });
        };

       if (hotelDomesticConfig.bookOverView) {
           relationInfoAjax();
       } else {
           var $relatePanel = $(relatePanel);
           scrollWaiter.start(200, relatePanel, relationInfoAjax);
       }
   }

     function DoFavHotel(obj , e , del, haslogin)
