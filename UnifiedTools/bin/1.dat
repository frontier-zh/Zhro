                            targetPrt.find("a.ico_favorite").remove();
                            targetPrt.html(targetPrt.html() + dataObj['IsFavList'][0][hotel]); 
                        }
                    }
                }
            });            
        } 
        if (e){
            e.returnValue = false;e.preventDefault && e.preventDefault();
            e.stopPropagation && e.stopPropagation();
            e.cancelBubble = true;
        }   
        return false;   
    }
      

